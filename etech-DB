-- Dumping structure for table eshop1.user_has_address
CREATE TABLE IF NOT EXISTS `user_has_address` (
  `id` int NOT NULL AUTO_INCREMENT,
  `user_email` varchar(100) NOT NULL,
  `line1` text NOT NULL,
  `line2` text NOT NULL,
  `city_id` int NOT NULL,
  PRIMARY KEY (`id`),
  KEY `FK_user_has_address_users` (`user_email`),
  KEY `FK_user_has_address_city` (`city_id`),
  CONSTRAINT `FK_user_has_address_city` FOREIGN KEY (`city_id`) REFERENCES `city` (`id`),
  CONSTRAINT `FK_user_has_address_users` FOREIGN KEY (`user_email`) REFERENCES `user` (`email`)
) ENGINE=InnoDB AUTO_INCREMENT=12 DEFAULT CHARSET=latin1;

